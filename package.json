{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma migrate deploy && next build",
    "start": "next start",
    "lint": "eslint",
    "test": "pnpm test:build && node --test --loader ./scripts/alias-loader.mjs .tmp/tests/**/*.test.js && pnpm test:clean",
    "test:build": "tsc -p tsconfig.tests.json",
    "test:clean": "node -e \"require('fs').rmSync('.tmp/tests',{recursive:true,force:true});\"",
    "test:ci": "pnpm test",
    "postinstall": "prisma generate"
  },
  "dependencies": {
