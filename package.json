    "@prisma/client": "^6.17.0",
    "csv-parse": "^6.1.0",
    "next": "15.5.4",
    "next-auth": "5.0.0-beta.29",
    "next-pwa": "^5.6.0",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-currency-input-field": "^4.0.3",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "react-is": "^19.2.0",
    "recharts": "^3.2.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
